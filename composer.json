{
    "name": "dvelopment/fastbill-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle for making calls to fastbill.com API",
    "keywords": ["symfony", "fastbill"],
    "homepage": "https://github.com/dVelopment/FastBillBundle",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bolzfabrik/fastbill"
        }
    ],
    "authors": [
        {
            "name": "Daniel Holzmann",
            "email": "d@velopment.at"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "dvelopment/fastbill": "dev-master#adc7ce6c2d818e10576ce5e953dfe18201509e80 as 1.0.0",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "symfony/class-loader": "~2.1"
    },
    "autoload": {
        "psr-0": {"DVelopment\\FastBillBundle": ""}
    },
    "minimum-stability": "dev",
    "target-dir": "DVelopment/FastBillBundle"
}
